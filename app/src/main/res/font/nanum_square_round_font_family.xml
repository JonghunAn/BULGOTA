<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <!--   logo -->
    <font
        android:font="@font/logo"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/logo"
        app:fontStyle="normal"
        app:fontWeight="400"
        />

    <!--   regular -->
    <font
        android:font="@font/nanum_square_round_regular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/nanum_square_round_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        />

    <!--    extra bold -->
    <font
        android:font="@font/nanum_square_round_extra_bold"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/nanum_square_round_extra_bold"
        app:fontStyle="normal"
        app:fontWeight="400"
        />

    <!--    bold -->
    <font
        android:font="@font/nanum_square_round_bold"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/nanum_square_round_bold"
        app:fontStyle="normal"
        app:fontWeight="400"
        />

    <!--    light -->
    <font
        android:font="@font/nanum_square_round_light"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/nanum_square_round_light"
        app:fontStyle="normal"
        app:fontWeight="400"
        />
</font-family>